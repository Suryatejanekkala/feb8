def isprime(n):
    x=n//2
    for i in range(2,n):
            if n%i==0:
                 return False
    return True   
t=int(input('enter t value'))
for i in range(t):
     x=input().split(' ')
     start=int(x[0])
     end=int(x[1])
     count=0
     for i in range(start+1,end):
                   if isprime(i):
                       count=count+1
     print(count)
